{
    "env": {
        "AIRTABLE_TOKEN": "@airtable_token"
    },
    "build": {
        "env": {
            "AIRTABLE_TOKEN": "@airtable_token"
        }
    },
    "routes": [
        {
            "src": "/api/(.*)",
            "headers": {
                "cache-control": "s-maxage=3600"
            },
            "dest": "/api/$1",
            "continue": true
        }
    ]
}
